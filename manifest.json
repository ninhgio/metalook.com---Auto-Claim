{
  "name": "Metalook.com - Auto Claim",
  "version": "1.0.0",
  "description": "Auto claim point",
  "manifest_version": 3,
  "author": "ninhgio@gmail.com",
  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "unlimitedStorage",
    "nativeMessaging"
  ],
  "content_scripts": [
    {
      "css": [],
      "js": [
        "content-script.js"
      ],
      "matches": [
        "https://metalook.com/*"
      ],
      "exclude_matches": [
      ]
    }
  ]
}
